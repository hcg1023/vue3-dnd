{
  "name": "vue3-dnd",
  "version": "0.0.8",
  "description": "Drag and Drop for Vue3",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "clean": "shx rm -rf dist/",
    "build_types": "tsc -b ./lib",
    "build_esm": "swc -C module.type=es6 -d dist/esm lib/",
    "build_cjs": "swc -C module.type=commonjs -d dist/cjs lib/",
    "build:swc": "run-s test clean && run-p build_types build_esm build_cjs",
    "build:vite": "vite build",
    "build": "run-s test clean build:swc",
    "prepare": "husky install",
    "prettier": "lint-staged",
    "prepublishOnly": "npm run build && standard-version"
  },
  "author": "hcg1023 <2293885211@qq.com>",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hcg1023/vue3-dnd"
  },
  "bugs": "https://github.com/hcg1023/vue3-dnd/issues",
  "license": "MIT",
  "dependencies": {
    "@react-dnd/invariant": "^3.0.0",
    "@react-dnd/shallowequal": "^3.0.0",
    "dnd-core": "^15.1.1",
    "fast-deep-equal": "^3.1.3"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "drag",
    "drop",
    "dnd",
    "vue",
    "vuejs",
    "vue3",
    "composition-api",
    "react-dnd",
    "draggable",
    "droppable",
    "drag-and-drop"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/vue": "^6.5.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react-dnd-html5-backend": "^15.1.2",
    "react-dnd-test-backend": "^15.1.1",
    "react-dnd-test-utils": "^15.1.1",
    "shx": "^0.3.4",
    "standard-version": "^9.3.2",
    "stylelint": "^14.5.3",
    "swc": "^1.0.11",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-dts": "^0.9.10",
    "vitest": "^0.6.0",
    "vue": "^3.2.31",
    "vue-eslint-parser": "^8.3.0",
    "vue-ref2reactive": "^0.0.3",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.{js,js,ts,tsx,vue}": [
      "prettier --write"
    ],
    "*.{less,css}": [
      "stylelint --fix"
    ]
  }
}
