{
  "name": "vue3-dnd",
  "version": "2.0.0",
  "description": "Drag and Drop for Vue3",
  "scripts": {
    "test": "vitest run",
    "clean": "shx rm -rf dist/",
    "build_types": "tsc",
    "build_esm": "swc -C module.type=es6 -d dist/esm src/",
    "build_cjs": "swc -C module.type=commonjs -d dist/cjs src/",
    "build:swc": "run-p build_types build_esm build_cjs",
    "build": "run-s switch:3 clean build:swc",
    "prepublishOnly": "npm run build",
    "switch:2": "vue-demi-switch 2 vue2",
    "switch:3": "vue-demi-switch 3"
  },
  "author": "hcg1023 <2293885211@qq.com>",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    "import": "./src/index.ts",
    "require": "./src/index.ts",
    "types": "./src/index.ts"
  },
  "publishConfig": {
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hcg1023/vue3-dnd"
  },
  "homepage": "https://hcg1023.github.io/vue3-dnd/",
  "bugs": "https://github.com/hcg1023/vue3-dnd/issues",
  "license": "MIT",
  "dependencies": {
    "@react-dnd/invariant": "^3.0.1",
    "@react-dnd/shallowequal": "^3.0.1",
    "dnd-core": "^15.1.2",
    "fast-deep-equal": "^3.1.3",
    "vue-demi": "^0.12.5"
  },
  "peerDependencies": {
    "vue": "^3.0.0-0 || ^2.6.0"
  },
  "files": [
    "dist",
    "README_ZH.md"
  ],
  "keywords": [
    "drag",
    "drop",
    "dnd",
    "vue",
    "vuejs",
    "vue3",
    "composition-api",
    "react-dnd",
    "draggable",
    "droppable",
    "drag-and-drop"
  ],
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.178",
    "@testing-library/vue": "^6.5.1",
    "@types/faker": "^5.5.9",
    "esno": "^0.14.1",
    "faker": "^5.5.3",
    "npm-run-all": "^4.1.5",
    "react-dnd-test-backend": "^15.1.2",
    "shx": "^0.3.4",
    "typescript": "^4.6.4",
    "vitest": "^0.12.9",
    "vue": "3"
  }
}
